      "import": {
        "types": "./dist/types/app/appform/index.d.ts",
        "default": "./dist/form.es.js"
      }
    },
    "./filemanager": {
      "import": {
        "types": "./dist/types/app/filemanager/index.d.ts",
        "default": "./dist/filemanager.es.js"
      }
    },
    "./style": {
      "import": {
        "types": "./dist/style.d.ts",
        "default": "./dist/style.css"
      }
    }
  },
